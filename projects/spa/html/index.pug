doctype html
html(lang='en', ng-app="app")
	head
		title Insert title here
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1")
		link(rel='stylesheet', href='../css/master.css')
		link(rel='stylesheet', href='https://opensource.keycdn.com/fontawesome/4.6.3/font-awesome.min.css')

	body(ng-controller="ctrl")
		header
			#logo
				h1 p
			#hamburger
				i.fa.fa-bars

		#view

		footer
			#up-down-container
				#left
					i.fa.fa-chevron-circle-up.up
					h1 LEFT EAR
						span(ng-model="leftHz") 130Hz
					i.fa.fa-chevron-circle-down.down
					#arrowUp
					#arrowDown
				#right
					h1 RIGHT EAR
						span(ng-model="leftHz") 130hz

					#arrowUp
					#arrowDown
			section.pause-play
				i.fa.fa-stop
				i.fa.fa-play
			section.current-hz
				h1.border Current Frequency
				h1.freq 6Hz





















		// Angular Cdn
		script(type="text/javascript", src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js")
		// jquery Cdn
		script(src="http://code.jquery.com/jquery-3.1.0.min.js")
		// Angular files
		script(type="text/javascript", src="../js/app.js")
		script(type="text/javascript", src="../js/ctrl.js")
		script(type="text/javascript", src="../js/srvc.js")
		script(type="text/javascript", src="../js/dir.js")
		// Personal etc js
		script(type="text/javascript", src="../js/script.js")
